{
  "name": "express-server",
  "author": "Nishant Kohli",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec 'ts-node -r tsconfig-paths/register src/index.ts'",
    "build": "tsc && tsc-alias",
    "start": "env-cmd -f .env.prod node dist/index.js",
    "prod": "yarn build && yarn start",
    "lint": "eslint --fix .",
    "seed:postgres": "env-cmd -f .env ts-node src/db/postgres/seeders/index.ts"
  },
  "dependencies": {
    "@csl/mongo-models": "*",
    "@csl/react-express": "*",
    "cors": "^2.8.5",
    "dotenv": "^16.4.4",
    "express": "^4.21.2",
    "fluent-ffmpeg": "^2.1.3",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.12.0",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "redis": "^4.7.0",
    "sequelize": "^6.37.5",
    "socket.io": "^4.8.1",
    "stytch": "^12.4.0",
    "uuid": "^11.0.5",
    "winston": "3.11.0"
  },
  "devDependencies": {
    "@nish1896/eslint-config": "^2.1.3",
    "@types/cors": "^2.8.17",
    "@types/express": "4.17.21",
    "@types/express-serve-static-core": "^4.17.33",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/multer": "1.4.12",
    "@types/node": "^20.11.17",
    "@types/uuid": "^10.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "nodemon": "^3.0.3",
    "tsc-alias": "^1.8.10",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
