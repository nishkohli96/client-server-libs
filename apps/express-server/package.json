    "@types/cors": "^2.8.17",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/multer": "2.0.0",
    "@types/node": "^22.14.1",
    "@types/papaparse": "^5.3.15",
    "@types/uuid": "^10.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^9.25.1",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.15",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
