{
  "name": "express-server",
  "author": "Nishant Kohli",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec 'ts-node -r tsconfig-paths/register src/index.ts'",
    "build": "tsc && tsc-alias",
    "start": "env-cmd -f .env.prod node dist/index.js",
    "prod": "yarn build && yarn start",
    "lint": "eslint --fix .",
    "seed:postgres": "env-cmd -f .env ts-node src/db/postgres/seeders/index.ts",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org nish1896 --project node-express dist && sentry-cli sourcemaps upload --org nish1896 --project node-express dist"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.767.0",
    "@aws-sdk/client-iam": "^3.758.0",
    "@aws-sdk/client-lambda": "3.758.0",
    "@aws-sdk/client-s3": "^3.758.0",
    "@aws-sdk/client-ses": "^3.758.0",
    "@aws-sdk/client-sns": "^3.758.0",
    "@aws-sdk/client-sqs": "^3.758.0",
    "@aws-sdk/client-ssm": "^3.759.0",
    "@aws-sdk/client-secrets-manager": "^3.758.0",
    "@aws-sdk/s3-request-presigner": "^3.758.0",
    "@csl/mongo-models": "*",
    "@csl/react-express": "*",
    "@sentry/cli": "^2.42.1",
    "@sentry/node": "^9.1.0",
    "@sentry/profiling-node": "^9.1.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.4.4",
    "dd-trace": "^5.37.1",
    "express": "^4.21.2",
    "fluent-ffmpeg": "^2.1.3",
    "ioredis": "^5.5.0",
    "mixpanel": "^0.18.0",
    "moment": "^2.30.1",
    "mongoose": "^8.7.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.12.0",
    "papaparse": "^5.5.2",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "redis": "^4.7.0",
    "sequelize": "^6.37.5",
    "socket.io": "^4.8.1",
    "stytch": "^12.4.0",
    "uuid": "^11.0.5",
    "winston": "3.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@nish1896/eslint-flat-config": "^2.0.2",
    "@sentry/cli": "^2.42.1",
    "@types/cors": "^2.8.17",
    "@types/express": "4.17.21",
    "@types/express-serve-static-core": "^4.17.33",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/multer": "1.4.12",
    "@types/node": "^22.13.10",
    "@types/papaparse":"^5.3.15",
    "@types/uuid": "^10.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^9.22.0",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2"
  }
}
