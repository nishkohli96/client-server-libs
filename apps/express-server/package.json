{
  "name": "express-server",
  "author": "Nishant Kohli",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec 'ts-node -r tsconfig-paths/register src/index.ts'",
    "build": "tsc && tsc-alias",
    "start": "env-cmd -f .env.prod node dist/index.js",
    "prod": "yarn build && yarn start",
    "lint": "eslint --fix .",
    "seed:postgres": "env-cmd -f .env ts-node src/db/postgres/seeders/index.ts",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org nish1896 --project node-express dist && sentry-cli sourcemaps upload --org nish1896 --project node-express dist"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.788.0",
    "@aws-sdk/client-iam": "^3.787.0",
    "@aws-sdk/client-lambda": "3.787.0",
    "@aws-sdk/client-s3": "^3.787.0",
    "@aws-sdk/client-secrets-manager": "^3.787.0",
    "@aws-sdk/client-ses": "^3.787.0",
    "@aws-sdk/client-sns": "^3.787.0",
    "@aws-sdk/client-sqs": "^3.787.0",
    "@aws-sdk/client-ssm": "^3.787.0",
    "@aws-sdk/s3-request-presigner": "^3.787.0",
    "@sentry/cli": "^2.43.0",
    "@sentry/node": "^9.13.0",
    "@sentry/profiling-node": "^9.13.0",
    "apn": "^2.2.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dd-trace": "^5.48.1",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "firebase-admin": "^13.5.0",
    "fluent-ffmpeg": "^2.1.3",
    "ioredis": "^5.6.1",
    "mixpanel": "^0.18.1",
    "moment": "^2.30.1",
    "mongoose": "^8.13.2",
    "multer": "^2.0.2",
    "mysql2": "^3.14.0",
    "papaparse": "^5.5.2",
    "pg": "^8.15.1",
    "pg-hstore": "^2.3.4",
    "redis": "^4.7.0",
    "sequelize": "^6.37.7",
    "socket.io": "^4.8.1",
    "stripe": "^18.0.0",
    "stytch": "^12.17.0",
    "ua-parser-js": "^2.0.6",
    "uuid": "^11.1.0",
    "winston": "3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@nish1896/eslint-flat-config": "^3.0.3",
